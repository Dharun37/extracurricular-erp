{
  "name": "erp-extracurricular-backend",
  "version": "1.0.0",
  "description": "Extra-Curricular Activity Module Backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:create": "node scripts/create-database.js",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/migrate-enhanced-schema.js",
    "db:seed": "node scripts/seed-enhanced-data.js",
    "db:init": "npm run db:create && npm run db:setup",
    "db:enhanced": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "erp",
    "extracurricular",
    "activities"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
